{
    "chains": [
        {
            "name": "base",
            "chain_id": 8453,
            "rpc_url_env_var": "BASE_RPC_URL",            
            "start_block": 26602741,
            "contracts": "contracts/base",
            "tokens": "tokens/base.json",
            "block_receipts_method": "eth_getBlockReceipts"
        }
    ],
    "raw_data_collection": {
        "channel_capacity": 5000,
        "factory_channel_capacity": 5000,
        "block_receipt_concurrency": 100,
        "parquet_block_range": 10000,
        "rpc_batch_size": 1000,
        "fields": {
            "block_fields": [
                "number",
                "timestamp",
                "transactions",
                "uncles"
            ],
            "receipt_fields": [
                "block_number",
                "timestamp",
                "transaction_hash",
                "from",
                "to"
            ],
            "log_fields": [
                "block_number",
                "timestamp",
                "transaction_hash",
                "log_index",
                "address",
                "topics",
                "data"
            ]
        },
        "contract_logs_only": false        
    },
    "transformations": {
        "database_url_env_var": "DATABASE_URL",
        "mode": {
            "batch_for_catchup": true,
            "catchup_batch_size": 10000
        },
        "handler_concurrency": 4,
        "max_batch_size": 1000
    }
}